<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>kotlin-actions-sdk</application>
    <version>dev</version> <!-- READ FROM ~/.gradle/gradle.properties BANJO_APP_ENGINE_VERSION-->

    <runtime>java8</runtime>
    <threadsafe>true</threadsafe>
    <automatic-scaling>
        <min-pending-latency>100ms</min-pending-latency>
        <max-pending-latency>500ms</max-pending-latency>
        <max-concurrent-requests>30</max-concurrent-requests>
    </automatic-scaling>
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
</appengine-web-app>
